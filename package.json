  },
  "dependencies": {
    "hoodie": "2.1.1",
    "hoodie-admin-dashboard": "^2.0.0",
    "hoodie-admin-dashboard-uikit": "^3.0.0",
    "hoodie-plugin-appconfig": "^2.0.1",
    "hoodie-plugin-email": "^1.0.0",
    "hoodie-plugin-users": "^2.2.2",
    "hoodie-server": "11.0.0",
    "lodash": "^3.10.1",
    "node-emoji": "^1.0.3",
    "nopt": "^3.0.3",
    "npmlog": "^1.2.1",
    "rc": "^1.1.1"
  },
  "devDependencies": {
    "browser-launcher": "^1.0.0",
    "browserify": "^11.0.1",
    "corsify": "^2.1.0",
    "envify": "^3.4.0",
    "hoodie-plugin-test": "hoodiehq/hoodie-plugin-test",
    "request": "^2.61.0",
    "semantic-release": "^4.3.5",
    "tap-finished": "0.0.1",
    "tap-spec": "^4.0.2",
    "tape": "^4.1.0"
  },
  "homepage": "https://github.com/hoodiehq/hoodie#readme",
  "hoodie": {
